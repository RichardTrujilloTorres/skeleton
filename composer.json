{
  "name": "lighthouse/skeleton",
  "description": "Lighthouse Framework Application Skeleton",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "framework",
    "skeleton",
    "mvc",
    "lighthouse"
  ],
  "require": {
    "php": "^8.2",
    "lighthouse/framework": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "repositories": [
    {"type": "vcs", "url": "https://github.com/RichardTrujilloTorres/http"},
    {"type": "vcs", "url": "https://github.com/RichardTrujilloTorres/container"},
    {"type": "vcs", "url": "https://github.com/RichardTrujilloTorres/middleware"},
    {"type": "vcs", "url": "https://github.com/RichardTrujilloTorres/router"},
    {"type": "vcs", "url": "https://github.com/RichardTrujilloTorres/view"},
    {"type": "vcs", "url": "https://github.com/RichardTrujilloTorres/error-handler"},
    {"type": "vcs", "url": "https://github.com/RichardTrujilloTorres/framework"}
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8000 -t public",
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
